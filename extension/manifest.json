{
  "name": "Manila",
  "manifest_version": 2,
  "version": "0.2",
  "content_scripts": [{
    "matches": [ "<all_urls>" ],
    "js": [ "jquery.js","set-tab.js" ]
  }],
  "browser_action": {
    "default_icon": "icon.png"
  },

  "icons": { "16": "icon_19.png",
         "48": "icon_48.png",
        "128": "icon_128.png" 
  },

  "chrome_url_overrides": {
      "newtab": "main.html"
  },

  "background": {
    "scripts": ["background.js"]
  }

}


